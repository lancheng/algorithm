#======================================================================================================================= Imports

#======================================================================================================================= Static

#======================================================================================================================= Classes
class Solution(object):
    def titleToNumber(self, s):
        """
        :type s: str
        :rtype: int
        """
        upperStr = s.upper()
        num = 0
        for letter in upperStr:
            num = num * 26 + ord(letter) - ord('A') + 1

        return num
#======================================================================================================================= Functions

